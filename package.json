{
  "name": "basic-ftp",
  "version": "2.14.3",
  "description": "FTP client for Node.js with support for explicit FTPS over TLS.",
  "main": "./dist/ftp",
  "scripts": {
    "test": "mocha",
    "tdd": "mocha --watch",
    "build": "babel lib --presets babel-preset-env --out-dir dist",
    "lint": "eslint lib/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/patrickjuchli/basic-ftp.git"
  },
  "author": "Patrick Juchli <patrickjuchli@gmail.com>",
  "license": "MIT",
  "keywords": [
    "ftp",
