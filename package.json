    "promise",
    "async",
    "await",
    "tls",
    "ipv6"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "devDependencies": {
    "eslint": "5.6.1",
    "mocha": "5.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.2",
    "babel-eslint": "^9.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ]
